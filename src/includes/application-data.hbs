<div class="fsa-m-t--s fsa-level fsa-level--grow-even">
	<div><span class="fsa-text-size--3">Year: <strong><span class="fsa-text-allcaps">2020</span></strong></span></div>
	<div><span class="fsa-text-size--3">State: <strong><span class="fsa-text-allcaps">South Dakota (46)</span></strong></span></div>
	<div><span class="fsa-text-size--3">County: <strong><span class="fsa-text-allcaps">Codington (029)</span></strong></span></div>
</div>

<div class="fsa-m-t--xs fsa-level fsa-level--grow-even">
	<div><span class="fsa-text-size--3">Producer: <strong><span class="fsa-text-allcaps" data-producer-name="{{ PRODUCER_NAME }}">{{ PRODUCER_NAME }}</span></strong></span></div>
	<div><span class="fsa-text-size--3">Commodity: <strong><span class="fsa-text-allcaps" data-commodity="{{ COMMODITY }}"></span>{{ COMMODITY }}</span></strong></span></div>
	<div><span class="fsa-text-size--3">Class: <strong><span class="fsa-text-allcaps" data-commodity-class="{{ COMMODITY_CLASS }}"></span>{{ COMMODITY_CLASS }}</span></strong></span></div>
</div>

<div class="fsa-m-t--xs fsa-level fsa-level--grow-even">
	<div><span class="fsa-text-size--3">Application Type: <strong><span class="fsa-text-allcaps" data-app-type="{{ APP_TYPE }}"></span>{{ APP_TYPE }}</span></strong></span></div>
	<div><span class="fsa-text-size--3">Application Number: <strong><span class="fsa-text-allcaps" data-app-number="{{ APP_NUMBER }}"></span>{{ APP_NUMBER }}</span></strong></span></div>
	<div><span class="fsa-text-size--3">Application Status: <strong><span class="fsa-text-allcaps" data-app-status="{{ APP_STATUS }}"></span>{{ APP_STATUS }}</span></strong></span></div>
</div>

<div class="fsa-m-t--xs fsa-m-b--s fsa-level fsa-level--grow-even">
	<div><span class="fsa-text-size--3">Reference Information: <strong><span class="fsa-text-allcaps" data-ref-info="{{ REF_INFO }}"></span>{{ REF_INFO }}</span></strong></span></div>
</div>

